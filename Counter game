char counterGame(long n) {
    int turns = 0;
    while (n != 1) {
        if ((n & (n - 1)) == 0) {
            n = n / 2;
        } else {
            long power = 1L << ((long)log2(n));
            n = n - power;
        }
        turns++;
    }

    if (turns % 2 == 0) {
        return "Richard";
    } else {
        return "Louise";
    }
}
